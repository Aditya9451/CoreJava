package com.collections.basics;

import com.practice.map.Employee;

public class InterfaceAndClass extends Employee implements interface1{

	@Override
	public void run() {
		System.out.println("Implemenattion method is running");	
	}
	
	public static void main(String[] args) {
	interface1 intt= new InterfaceAndClass();
	intt.run();
	
	Employee emp= new InterfaceAndClass();
	emp.run();
	
	InterfaceAndClass in= new InterfaceAndClass();
	in.run();
	
	Employee emp2= new Employee();
	emp2.run();
	
	
	}
}
